library(XML)
library(RCurl)
library(httr)
urlPath <- "https://cafenomad.tw/taipei/list"
temp    <- getURL(urlPath, encoding = "big5")
xmldoc  <- htmlParse(temp)
title   <- xpathSApply(xmldoc, "//td[1]", xmlValue)
seat    <- xpathSApply(xmldoc, "//td[2]", xmlValue)
quiet   <- xpathSApply(xmldoc, "//td[3]", xmlValue)
coffee  <- xpathSApply(xmldoc, "//td[4]", xmlValue)
price   <- xpathSApply(xmldoc, "//td[5]", xmlValue)
alldata <- data.frame("title","seat","quiet","coffee","price")
write.table(alldata,"coffee.csv") #你會在此Script資料夾下得到一個.csv
